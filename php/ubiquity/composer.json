{
	"require" : {
		"php" : "^7.1",
		"phpmv/ubiquity" : "dev-master"
	},
	"require-dev": {
		"mindplay/annotations": "^1.3",
		"phpmv/ubiquity-dev": "dev-master",
		"phpmv/ubiquity-devtools": "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"" : "app/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"preferred-install": "dist"
	},
	"repositories": {
		"packagist": {
			"type": "composer",
			"url": "https://packagist.phpcomposer.com"
		}
	}
}
